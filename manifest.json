{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "React Chrome Extension",
  "description": "This is a Chrome extension built with React and TypeScript",
  "background": {
    "service_worker": "/background.js"
  },
  "action": {
    "default_title": "Click me"
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "favicon"
  ]
}